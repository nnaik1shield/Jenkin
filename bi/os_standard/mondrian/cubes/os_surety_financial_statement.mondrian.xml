    </Measure>
    <CalculatedMember name="Avg_Credit_Score_Text" caption="Avg Credit Score Text" description="Avg Credit Score" dimension="Measures" visible="false">
      <Formula>
        <![CDATA[Case 
				  When [Measures].[Avg_Credit_Score] >= 97.0 AND [Measures].[Avg_Credit_Score] <= 100.0 Then 'AAA'
				  When [Measures].[Avg_Credit_Score] >= 93.0 AND [Measures].[Avg_Credit_Score] <= 96.0 Then 'AA'
				  When [Measures].[Avg_Credit_Score] >= 90.0 AND [Measures].[Avg_Credit_Score] <= 92.0 Then 'A'
				  When [Measures].[Avg_Credit_Score] >= 87.0 AND [Measures].[Avg_Credit_Score] <= 89.0 Then 'BBB'
				  When [Measures].[Avg_Credit_Score] >= 83.0 AND [Measures].[Avg_Credit_Score] <= 86.0 Then 'BB'
				  When [Measures].[Avg_Credit_Score] >= 80.0 AND [Measures].[Avg_Credit_Score] <= 82.0 Then 'B'
				  When [Measures].[Avg_Credit_Score] >= 77.0 AND [Measures].[Avg_Credit_Score] <= 79.0 Then 'CCC'
				  When [Measures].[Avg_Credit_Score] >= 73.0 AND [Measures].[Avg_Credit_Score] <= 76.0 Then 'C'
				  When [Measures].[Avg_Credit_Score] >= 70.0 AND [Measures].[Avg_Credit_Score] <= 72.0 Then 'D'
				  When [Measures].[Avg_Credit_Score] >= 67.0 AND [Measures].[Avg_Credit_Score] <= 69.0 Then 'DDD'
				  When [Measures].[Avg_Credit_Score] >= 63.0 AND [Measures].[Avg_Credit_Score] <= 66.0 Then 'DD'
				  When [Measures].[Avg_Credit_Score] >= 60.0 AND [Measures].[Avg_Credit_Score] <= 62.0 Then 'D'
				  Else 'N/A'
			End]]>
      </Formula>
    </CalculatedMember>
