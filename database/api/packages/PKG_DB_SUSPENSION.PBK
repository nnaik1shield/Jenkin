        sp_get_active_susp_objects 
					(
						in_session_id,
						in_transaction_id,
						v_master_policy_id,
						v_responsible_billing_account,
						v_susp_child_list,
						3
					);
--		pkg_os_object_search.sp_object_children_of_type_get
--                       (
--                            in_session_id,
--                            in_transaction_id,
--                            v_master_policy_id ,
--                            gObjType_PolicySuspension,--3193646
