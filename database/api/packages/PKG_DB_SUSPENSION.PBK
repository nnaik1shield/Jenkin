                                                     in_transaction_id,
                                                     v_policy_id,
                                                     pkg_os_policy_transaction.gbv_PolicyEffectiveDate
                                                ),pkg_os_constant.DragonDateFormatDefault);
                v_policy_expiration_date :=   to_date(pkg_os_object_io.fn_object_bv_get
                                                (
