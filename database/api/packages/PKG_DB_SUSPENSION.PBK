                   --OSPRODUCT-2012
               v_bv_table(gbv_Suspension_Exp_Date).business_variable_value :=     pkg_os_object_io.fn_object_bv_get
                                                                                                    (
                                                                                                         in_session_id,
                                                                                                         in_transaction_id,
