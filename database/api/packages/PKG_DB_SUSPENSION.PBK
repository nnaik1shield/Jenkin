                                      );
     v_curr_policy_term            :=   pkg_os_object_io.fn_object_bv_get
                                        (
                                             in_session_id,
                                             in_transaction_id,
                                             v_policy_id,
                                             gbv_CurrentTermFromRolledDate
                                        ); --OSBILLING-7390
     If v_Session_Control.Glogging_Full

     then
       pkg_os_logging.sp_log
       (
         in_session_id,
         in_transaction_id,
         V_Procedure_Name,
